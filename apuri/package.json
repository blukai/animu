{
  "name": "animu",
  "version": "0.0.0",
  "scripts": {
    "test": "jest --watch",
    "dev:main": "cross-env NODE_ENV=development electron ./main/main.js",
    "dev:renderer":
      "cross-env NODE_ENV=development webpack-dev-server --config ./renderer/.webpack/config.js",
    "start":
      "concurrently --kill-others \"npm run dev:renderer\" \"npm run dev:main\"",
    "build:renderer":
      "cross-env NODE_ENV=production webpack --config ./renderer/.webpack/config.js"
  },
  "author": "blukai",
  "dependencies": {
    "dexie": "^2.0.1",
    "electron": "1.7.11",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "styled-components": "^3.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.39",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.39",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "@babel/preset-react": "^7.0.0-beta.39",
    "babel-jest": "^22.1.0",
    "babel-loader": "^8.0.0-beta.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.1.4",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "why-did-you-update": "^0.1.0"
  },
  "jest": {
    "setupFiles": ["./renderer/.jest/setup.js"]
  },
  "babel": {
    "env": {
      "test": {
        "presets": ["@babel/preset-env", "@babel/preset-react"]
      }
    }
  }
}
